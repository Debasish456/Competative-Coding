for i in range(int(input())):
    n,x,k=map(int, input().split())
    A=list(map(int, input().split()))
    B=list(map(int, input().split()))
    count=0
    for  i in range(n):
        if abs(A[i]-B[i])<=k:
            count+=1
    if count>=x:
        print("YES")
    else:
        print("NO")
            
